{
  "name": "swagger-ai-search",
  "version": "1.0.2",
  "description": "AI-powered endpoint search for Swagger UI. Works with NestJS and Express.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "default": "./dist/express.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["dist/index.d.ts"],
      "express": ["dist/express.d.ts"]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp -r src/assets dist/assets",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "swagger",
    "openapi",
    "ai",
    "search",
    "anthropic",
    "claude",
    "nestjs",
    "express",
    "api-documentation"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/swagger": ">=7.0.0",
    "express": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": { "optional": true },
    "@nestjs/core": { "optional": true },
    "@nestjs/swagger": { "optional": true },
    "express": { "optional": true }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/swagger": "^11.2.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typescript": "^5.3.0"
  }
}
